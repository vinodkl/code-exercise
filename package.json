    "material-ui": "^0.16.7",
    "npm": "^4.1.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-responsive": "^4.1.1",
    "redux-thunk": "^2.1.0"
  },
